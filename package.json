{
  "name": "library-webservices-status",
  "private": true,
  "version": "0.0.2",
  "description": "Library Webservices Status System UI",
  "repository": "https://github.com/TAMULib/LibraryServiceStatusSystemUI.git",
  "license": "MIT",
  "scripts": {
    "preinstall": "npm install -g grunt-cli",
    "postinstall": "grunt",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "stop": "wvr server --stop",
    "start": "wvr server -b /status-system -p 80 --prod --uid lws-webserver",
    "start-dev": "wvr server -b /status-system --uid lws-webserver"
  },
  "dependencies": {
    "angular-ui-tinymce": "0.0.19",
    "core": "git+https://github.com/TAMULib/Weaver-UI-Core.git#2.0.x-webservice",
    "ng-csv": "0.3.6",
    "ng-table": "3.0.1",
    "tinymce": "4.6.5"
  },
  "devDependencies": {
    "grunt": "1.0.1",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-jshint": "1.1.0",
    "grunt-contrib-symlink": "1.0.0",
    "grunt-contrib-uglify": "3.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sass": "2.0.0",
    "grunt-usemin": "3.1.1",
    "jshint-stylish": "2.2.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.0",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "protractor": "5.1.2"
  }
}
